import java.util.*;
class string{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the size");
        int size=sc.nextInt();
        String name[]=new String[size];
        int tolength=0;
        for(int i=0;i<size;i++){
             name[i]=sc.next();
             tolength+=name[i].length();
        }
        System.out.println(tolength);
    }
}
