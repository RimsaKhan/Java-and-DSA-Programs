import java.util.*;
class string{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
            String str=sc.next();
            String reversedstring="";
            for(int i=str.length()-1;i>=0;i--){    
            reversedstring+=str.charAt(i);
            }
            System.out.println(reversedstring);   
    }
}
