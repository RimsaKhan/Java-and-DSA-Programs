import java.util.*;
class program{
public static void main(String[] args) {
    int arr[]={1,2,3,3,4,3,3};
    int n=arr.length;
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter the number u want to count");
    int num=sc.nextInt();
    int count=0;
    for(int i=0;i<n;i++){
        if(num==arr[i]){
            count++; 
        }
    }
    System.out.println("the number of times"+num+"occurs is"+" "+count);
}
}
